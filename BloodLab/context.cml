Domain LabManagement {
	Subdomain Testing
    Subdomain Results
    Subdomain Administration

}

BoundedContext SystemManagement implements  Administration, Testing, Results {
	Aggregate Person {
		Entity Person {
			aggregateRoot
			-PersonId id
			-Cpf cpf
			String name
			-Email contactEmail
			Date dateOfBirth
		}
		
		ValueObject PersonId{
			int id
		}
		
		ValueObject Cpf {
			String cpf
		}
		
		ValueObject Email{
			String emailText
		}
	}
	
	
	Aggregate Patient {
		Entity Patient extends Person{
			aggregateRoot
			-PatientId id
			List<String> patientConditions
			-List<ResultId> results // ReferÃªncia aos IDs dos testes
		}
		ValueObject PatientId {
			int id
		}
	}
	
	
	Aggregate Doctor {
		Entity Doctor extends Person{
			aggregateRoot
			-DoctorId id
		}
		ValueObject DoctorId {
			int id
		}
	}
	
	
	Aggregate Test {
		Entity Test {
			aggregateRoot
			-TestId id
			String name
			String descriptionText
		}
		
		ValueObject TestId {
			int id
		}
	}
	
	
	Aggregate Appointment {
		Entity Appointment {
			aggregateRoot
			-AppointmentId id
			Date dateOfAppointment
			- PatientId patientId
			- DoctorId doctorId
		}
		
		ValueObject AppointmentId{
			int id
		}
	}
	
	
	Aggregate Result {
		Entity Result {
			aggregateRoot
			-ResultId id
			String contentOfResult
			-TestId test
		}
		
		ValueObject ResultId {
			int id
		}
	
	}
}

